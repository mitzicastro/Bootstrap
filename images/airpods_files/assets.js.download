          && window._evolv_5g_planJson.payload.hasOwnProperty("pageData")
          && window._evolv_5g_planJson.payload.pageData.hasOwnProperty("lines")
        ) {
          startControl();
          clearInterval(window._evolv_5g_interval);
          window._evolv_5g_interval = null;
        }
